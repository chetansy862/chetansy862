    \resumeProjectHeading
      {\textbf{\large{Content Classifier for Email}} $|$ \large{Python, Scikit-learn}}{May-2024}
      \vspace{-11pt}
      \resumeItemListStart
        \resumeItem{\normalsize{
        Trained a Machine Learning model to automatically classify incoming emails into categories.}}
        \resumeItem{\normalsize{
         Utilized Kaggle dataset and Scikit-learn for model development, training, and evaluation.}}
         \resumeItem{\normalsize{
         Achieved over 92\% accuracy on the validation dataset.}}
      
\resumeSubHeadingListEnd

  \resumeProjectHeading
      {\textbf{\large{License Plate Recognition}} $|$ \large{OpenCV, Python, YOLOv8, OCR}}{Apr-2024}
      \vspace{-11pt}
      \resumeItemListStart
          \resumeItem{\normalsize{
          Developed a system using OpenCV for real-time license plate recognition from vehicle images.}}
